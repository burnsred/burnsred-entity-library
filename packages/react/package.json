  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "test": "jest --config package/jest.config.js",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint '{lib,src}/**/*.{js,jsx}' --config package/stylelint.config.js",
    "lint:js": "eslint -c package/eslint.config.js --ext .js --ext .jsx src lib",
    "lint:flow": "npm run flow",
    "start": "webpack-dev-server --mode development --progress --inline --hot --content-base dist --config package/webpack.config.js",
    "build": "webpack --mode production --progress --config package/webpack.config.js",
    "deploy": "npm run build && npm run storybook:build",
    "serve-dist": "browser-sync start --server dist",
    "storybook": "start-storybook -c package/storybook -p 6006",
    "storybook:build": "build-storybook -c package/storybook -o dist/storybook"
  },
  "engines": {
    "node": "10.7.0",
    "npm": "6.1.0"
  },
