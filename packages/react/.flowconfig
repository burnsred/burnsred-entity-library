[ignore]
.*/node_modules/styled-components/.*
.*/node_modules/stylelint/.*
.*/node_modules/radium/.*
.*/node_modules/immutable/.*

[include]

[libs]

[options]
module.name_mapper.extension='ico' -> '<PROJECT_ROOT>/flow-typed/flow/WebpackAsset.js.flow'
module.name_mapper.extension='txt' -> '<PROJECT_ROOT>/flow-typed/flow/WebpackAsset.js.flow'
module.name_mapper.extension='css' -> '<PROJECT_ROOT>/flow-typed/flow/CSSModule.js.flow'
module.name_mapper='^assets\/\(.*\)$' -> '<PROJECT_ROOT>/assets/\1'
module.name_mapper='^components\/\(.*\)$' -> '<PROJECT_ROOT>/src/components/\1'
module.name_mapper='^components$' -> '<PROJECT_ROOT>/src/components'
module.name_mapper='^entities\/\(.*\)$' -> '<PROJECT_ROOT>/src/entities/\1'
module.name_mapper='^entities$' -> '<PROJECT_ROOT>/src/entities'
module.name_mapper='^lib\/\(.*\)$' -> '<PROJECT_ROOT>/lib/\1'
module.name_mapper='^lib$' -> '<PROJECT_ROOT>/lib'
module.name_mapper='^settings$' -> '<PROJECT_ROOT>/package/settings.config.js'
module.name_mapper='^store\/\(.*\)$' -> '<PROJECT_ROOT>/src/store/\1'
module.name_mapper='^store$' -> '<PROJECT_ROOT>/src/store'
module.name_mapper='^styles\/\(.*\)$' -> '<PROJECT_ROOT>/src/styles/\1'
module.name_mapper='^styles$' -> '<PROJECT_ROOT>/src/styles'

[lints]
