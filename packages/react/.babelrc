{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "edge": "17",
          "firefox": "60",
          "chrome": "67",
          "safari": "11.1"
        },
        "useBuiltIns": "usage"
      }
    ],
    "@babel/preset-react",
    "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-optional-chaining",
  ],
  "env": {
    "development": {
      "plugins": [
        "react-flow-props-to-prop-types",
        "@babel/plugin-transform-react-jsx"
      ]
    },
    "test": {},
    "production": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    }
  }
}
