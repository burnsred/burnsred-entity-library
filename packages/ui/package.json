    "lodash": "4.17.11",
    "material-design-icons": "3.0.1",
    "prop-types": "15.6.2",
    "react-emotion": "9.2.10",
    "react-intl": "2.6.0"
  },
  "peerDependencies": {
    "react": "16.5.1",
    "react-router-dom": "4.3.1"
  }
